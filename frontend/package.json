{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.11.0"
  },
  "scripts": {
    "dev": "rimraf ./node_modules/.vite && vite --force",
    "build": "tsc && NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "format": "prettier --write src",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts ./src",
    "lint:fix": "eslint --fix --ext .jsx,.js,.tsx,.ts ./src",
    "oapigen": "cd ../ && make oapigen",
    "oapigen:api": "cd ../ && make oapigen-api",
    "oapigen:administrator-api": "cd ../ && make oapigen-administrator-api",
    "orval": "npm run clean:orval && orval --config ./orval.config.ts",
    "orval:api": "npm run clean:orval:api && orval --project api",
    "orval:backend-api": "npm run clean:orval:backend-api && orval --project backend-api",
    "orval:administrator-api": "npm run clean:orval:administrator-api && orval --project administrator-api",
    "clean:orval": "rimraf ./src/orval",
    "clean:orval:api": "rimraf ./src/orval/models/api ./src/orval/api.ts",
    "clean:orval:backend-api": "rimraf ./src/orval/models/backend-api ./src/orval/backend-api.ts",
    "clean:orval:administrator-api": "rimraf ./src/orval/models/administrator-api ./src/orval/administrator-api.ts"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fluentui/react-icons-mdl2-branded": "^1.2.84",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.145",
    "@mui/material": "^5.14.6",
    "@mui/x-charts": "^7.4.0",
    "@mui/x-date-pickers": "^7.3.2",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-spring/web": "^9.7.1",
    "@sentry/react": "^8.27.0",
    "@sentry/vite-plugin": "^2.22.3",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "dompurify": "^3.0.1",
    "framer-motion": "^11.0.14",
    "hast": "^1.0.0",
    "hast-util-to-mdast": "^10.1.0",
    "launchdarkly-react-client-sdk": "^3.0.10",
    "mdast-util-gfm": "^3.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "ndjson-readablestream": "^1.0.9",
    "notistack": "^3.0.1",
    "orval": "^6.28.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.2",
    "react-icons": "^4.12.0",
    "react-markdown": "^9.0.0",
    "react-router-dom": "^6.8.1",
    "react-select": "^5.7.4",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.5.1",
    "remark-gfm": "^4.0.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@types/dompurify": "^2.4.0",
    "@types/node": "^20.11.5",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-syntax-highlighter": "^15.5.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "typescript": "^4.9.3",
    "vite": "^5.2.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "volta": {
    "node": "20.11.0"
  }
}
